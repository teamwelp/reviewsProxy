<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="<%=`${photoGalleryURL}/style.css`%>">
    <link rel="stylesheet" type="text/css" href="/proxy-style.css">
    <link rel="icon" type="image/png" href="/favicon.ico">
    <title><%-businessName%> - <%=reviewCount%> Reviews</title>
  </head>
  <body>
    <% include ./partials/header %>
    <div id="header-container">
      <div id="businessInfoHeader">Biz info header goes here</div>
      <div id="photoGallery">photogalleery</div>
    </div>
    <div id="content-container">
      <div id="review-container">
        <div id="related-top"></div>
        <div id="reviews">reviews</div>
      </div>
      
      <div id="sidebar-container">
        <div id="businessInfoSidebar">Biz info sidebar goes here</div> 
        <div id="related-side"></div>
        <div id="people-viewed"></div>
      </div>

    </div>
    <script>
      window.REVIEWS_URL = '<%=reviewsURL%>';
      window.BUSINESS_INFO_URL = '<%=photoGalleryURL%>';
      window.RELATED_URL = '<%=relatedURL%>';
    </script>
    <script src="<%=`${businessInfoURL}/biz/200/bundle.js`%>"></script>
    <script src="<%=`${photoGalleryURL}/dist/bundle.js`%>"></script>
    <script src="<%=`${reviewsURL}/dist/bundle.js`%>"></script>
    <script src="<%=`${relatedURL}/dist/bundle.js`%>"></script>

  </body>
</html>
